<script><![CDATA[(function executeRule(current, previous) {
	
	//Provide an extra warning if the user is escalated to "security_admin" as scripts run with this privilege
	//can do a lot of damage if bad code is run.
	if (gs.hasRole("security_admin")) {
		gs.addErrorMessage("WARNING: YOU ARE CURRENTLY ESCALATED TO security_admin. Any scripts executed will run with SECURITY_ADMIN level permission and could do serious harm if executed carelessly. USE THIS TOOL AT YOUR OWN RISK.");
	} else {
		gs.addInfoMessage("NOTE: Any scripts executed from this interface will run with admin permissions.");
	}

})(current, previous);]]></script>
